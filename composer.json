{
    "name": "dymecki/algoteque",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "require": {
        "symfony/http-foundation": "^5.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.5",
        "vlucas/phpdotenv": "^4.1",
        "monolog/monolog": "^2.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "^9.0"
    }
}
